script.
  let progress = 0;
  data = !{JSON.stringify(data)};
  data = JSON.parse(data);
  ppic = !{JSON.stringify(ppic)};
  pname = !{JSON.stringify(pname)};
  pdate = !{JSON.stringify(pdate)};
  document.getElementById("mobile-pic").src = ppic
  document.getElementById("user-pic").src = ppic
  document.getElementById("page-name").innerHTML = pname + "'s Progress | Cloud Skills Boost"
  document.getElementById("welcome").innerHTML =  "Welcome, " + pname
  document.getElementById("progress").innerHTML =  "Qwiklabs "+ pdate  +" <br/> Here is your progress: "
  data.forEach(element => {
  const li = document.createElement('li');
  var col = "green"
  var tcol = "white"
  if(element.state == "Incomplete"){
  progress = progress + 1
  col = "red"
  tcol = "red"
  }
  li.innerHTML = `<li>
  <a href="${element.url}" target="_blank" class="block hover:backdrop-blur-[2px]">
  <div class="px-4 py-4 sm:px-6">
  <div class="flex items-center justify-between">
  <p class="truncate text-sm font-medium text-${col}-600">${element.title}</p>
  <div class="ml-2 flex flex-shrink-0">
  <p class="inline-flex rounded-full bg-${col}-600 px-4 text-xs font-semibold leading-7 text-black-800">${element.state}</p>
  </div>
  </div>
  <div class="mt-2 text-white sm:flex sm:justify-between">
  <div class="mt-2 flex items-center text-sm text-${tcol}-500 sm:mt-0">
  <svg class="mr-1.5 h-5 w-5 flex-shrink-0 text-gray-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
  <path fill-rule="evenodd" d="M5.75 2a.75.75 0 01.75.75V4h7V2.75a.75.75 0 011.5 0V4h.25A2.75 2.75 0 0118 6.75v8.5A2.75 2.75 0 0115.25 18H4.75A2.75 2.75 0 012 15.25v-8.5A2.75 2.75 0 014.75 4H5V2.75A.75.75 0 015.75 2zm-1 5.5c-.69 0-1.25.56-1.25 1.25v6.5c0 .69.56 1.25 1.25 1.25h10.5c.69 0 1.25-.56 1.25-1.25v-6.5c0-.69-.56-1.25-1.25-1.25H4.75z" clip-rule="evenodd" />
  </svg>
  <p>
  <time>${element.date}</time>
  </p>
  </div>
  </div>
  </div>
  </a>
  </li>`;
  document.querySelector('#list').appendChild(li);
  });
  setTimeout(() => {
    document.getElementById("progress-bar").innerText = data.length - progress + "/" + data.length + "-" + (data.length - progress) / data.length * 100 + "%"
    document.getElementById("progress-bar").style.width = (data.length - progress) / data.length * 100 + "%";
  },1000)
  const googleCorlors = ["#3e7bf4", "#26b643", "#ebb105", "#e33729"]
  setInterval(() => {
    document.getElementById("progress-bar").style.backgroundColor = googleCorlors[Math.floor(Math.random() * googleCorlors.length)]
  }, 1000)
  window.addEventListener('DOMContentLoaded', (event) => {
      particlesJS.load('particles-js', 'particles.json', function() {
        console.log('callback - particles.js config loaded');
      });
    })
